{
  "name": "typestub-node-lmdb",
  "version": "0.3.1",
  "description": "Typescript wrapper around node-lmdb, with some improvement.",
  "keywords": [
    "lmdb",
    "node-lmdb",
    "typescript",
    "typing",
    "typestub"
  ],
  "author": "Beeno Tung <aabbcc1241@yahoo.com.hk> (https://beeno-tung.surge.sh)",
  "license": "BSD-2-Clause",
  "main": "dist/lmdb.js",
  "types": "dist/lmdb.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register **/*.spec.ts",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "postformat": "tslint -p . --fix",
    "prebuild": "npm run clean",
    "build": "tsc",
    "pretest": "npm run format && npm run build && mkdir -p data",
    "prepublishOnly": "npm run test"
  },
  "husky": {
    "hook": {
      "pre-commit": "npm run format"
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/typestub-node-lmdb.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/typestub-node-lmdb/issues"
  },
  "homepage": "https://github.com/beenotung/typestub-node-lmdb#readme",
  "files": [
    "dist"
  ],
  "dependencies": {
    "node-lmdb": "^0.9.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/mocha": "^5.2.7",
    "@types/node": "*",
    "chai": "^4.3.4",
    "husky": "^3.1.0",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.2.3"
  }
}
